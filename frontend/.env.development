# Configuración para desarrollo
REACT_APP_ENV=development
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_VERSION=$npm_package_version

# Configuración de PWA
REACT_APP_PWA_NAME=AI Calorie Counter (Dev)
REACT_APP_PWA_SHORT_NAME=CalorieAI Dev
REACT_APP_PWA_DESCRIPTION=Contador de calorías con inteligencia artificial - Desarrollo

# Configuración de análisis
REACT_APP_ANALYTICS_ENABLED=false

# Configuración de logging
REACT_APP_LOG_LEVEL=debug

# Configuración de cache
REACT_APP_CACHE_VERSION=dev

# Configuración de Service Worker
REACT_APP_SW_UPDATE_POPUP=false

# Configuración de desarrollo
REACT_APP_DEBUG_MODE=true